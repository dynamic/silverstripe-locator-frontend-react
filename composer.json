{
  "name": "dynamic/silverstripe-locator-react",
  "description": "A front end built in react for the SilverStripe Locator Module.",
  "keywords": [
    "silverstripe",
    "map",
    "locator",
    "dynamic",
    "geocode",
    "google maps",
    "location"
  ],
  "type": "silverstripe-vendormodule",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Dynamic",
      "email": "dev@dynamicagency.com",
      "homepage": "http://www.dynamicagency.com"
    }
  ],
  "require": {
    "silverstripe/recipe-cms": "^1.0 || ^4.0",
    "silverstripe/vendor-plugin": "^1.0",
    "dynamic/silverstripe-locator": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "squizlabs/php_codesniffer": "*"
  },
  "config": {
    "process-timeout": 600
  },
  "autoload": {
    "psr-4": {
      "Dynamic\\Locator\\React\\": "src/",
      "Dynamic\\Locator\\React\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "expose": [
      "client",
      "css"
    ],
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "scripts": {
    "lint": "vendor/bin/phpcs src/ tests/",
    "lint-clean": "vendor/bin/phpcbf src/ tests/"
  }
}
