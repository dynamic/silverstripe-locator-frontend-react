{
  "name": "locator",
  "version": "0.0.1",
  "description": "SilverStripe Locator Module. Show locations on a map. Search by geoposition.",
  "main": "client/src/js/boot/index.js",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js -p --progress",
    "build:dev": "webpack --config webpack.config.dev.js --progress",
    "watch": "webpack --config webpack.config.dev.js --progress --watch",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "test:coverage:watch": "jest --coverage --watchAll",
    "lint": "eslint webpack.config.dev.js webpack.config.prod.js client/src/js client/src/tests"
  },
  "keywords": [
    "locator",
    "graphql",
    "silverstripe",
    "react",
    "redux"
  ],
  "author": "Dynamic <dev@dynamicagency.com>",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.2",
    "@fortawesome/react-fontawesome": "0.0.17",
    "axios": "^0.17.1",
    "bootstrap": "^4.0.0-beta.2",
    "es6-promise": "^4.1.1",
    "handlebars": "^4.0.11",
    "history": "^4.7.2",
    "html-to-react": "^1.3.1",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.6.0",
    "querystring": "^0.2.0",
    "react": "^16.1.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.1.1",
    "react-google-maps": "^9.2.2",
    "react-redux": "^5.0.6",
    "react-virtualized": "^9.12.0",
    "redux": "^3.7.2",
    "redux-promise-middleware": "^4.4.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "axios-mock-adapter": "^1.10.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-import-resolver-node": "^0.3.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "jest-enzyme": "^4.0.1",
    "node-sass": "^4.6.1",
    "postcss-loader": "^2.0.8",
    "react-test-renderer": "^16.1.1",
    "remote-redux-devtools": "^0.5.12",
    "resolve-url-loader": "^2.2.0",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1"
  },
  "browserslist": "> 1%",
  "jest": {
    "roots": [
      "<rootDir>/client/src/",
      "<rootDir>/client/tests/"
    ],
    "collectCoverageFrom": [
      "**/client/src/**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/client/src/tests/"
    ],
    "moduleDirectories": [
      "node_modules",
      "client/src/js"
    ],
    "setupFiles": [
      "<rootDir>/client/src/tests/shim.js",
      "<rootDir>/client/src/tests/setup.js"
    ]
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-2"
    ]
  }
}
